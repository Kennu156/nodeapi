
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book {
  id Int @id @default(autoincrement())
  title String
  description String @db.Text
  year Int
  author String
  publisher String
  created_at DateTime @default(now())
  update_at DateTime @updatedAt 
}